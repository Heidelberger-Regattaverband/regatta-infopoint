<mvc:View controllerName="sap.ui.demo.walkthrough.controller.InvoiceList" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
  <List headerText="{i18n>invoiceListTitle}" class="sapUiResponsiveMargin" width="auto" items="{heat>/}" growing="true" growingScrollToLoad="true">
    <items>
      <ObjectListItem
        title="{heat>race_number} {heat>round_code}{heat>division_number} - {heat>race_short_label} - {heat>race_long_label}"
      />
    </items>
  </List>
</mvc:View>