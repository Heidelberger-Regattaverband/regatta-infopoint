<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem
				text="{i18n>common.state}"
				key="cancelled"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>common.raceDriven}" key="cancelled___EQ___false" />
					<ViewSettingsItem text="{i18n>common.raceCancelled}" key="cancelled___EQ___true" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
				text="Price"
				key="Price"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="Less Than 100" key="Price___LE___100___X" />
					<ViewSettingsItem text="Between 100 and 1000" key="Price___BT___100___1000" />
					<ViewSettingsItem text="More Than 1000" key="Price___GT___1000___X" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>