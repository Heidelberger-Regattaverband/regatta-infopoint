<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem
				text="{i18n>common.state}"
				key="cancelled"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>common.raceDriven}" key="cancelled___EQ___false" />
					<ViewSettingsItem text="{i18n>common.raceCancelled}" key="cancelled___EQ___true" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
				text="{i18n>common.distance}"
				key="distance"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="1000m" key="distance___EQ___1000" />
					<ViewSettingsItem text="1500m" key="distance___EQ___1500" />
					<ViewSettingsItem text="350m" key="distance___EQ___350" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
				text="{i18n>common.lightweight}"
				key="lightweight"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>common.yes}" key="lightweight___EQ___true" />
					<ViewSettingsItem text="{i18n>common.no}" key="lightweight___EQ___false" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
				text="{i18n>common.seeded}"
				key="seeded"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>common.yes}" key="seeded___EQ___true" />
					<ViewSettingsItem text="{i18n>common.no}" key="seeded___EQ___false" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>