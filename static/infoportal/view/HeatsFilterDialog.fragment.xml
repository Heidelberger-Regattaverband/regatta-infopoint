<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
  xmlns:cust="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
  <ViewSettingsDialog confirm="onHandleFilterDialogConfirm">
    <filterItems>
      <ViewSettingsFilterItem text="{i18n>common.day}" key="distance" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.saturday}" key="weekday___EQ___6" />
          <ViewSettingsItem text="{i18n>common.sunday}" key="weekday___EQ___7" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.boatClass}" key="gender">
        <items>
          <ViewSettingsItem text="Einer (1x)" key="race/boatClass/abbreviation___EQ___1x" />
          <ViewSettingsItem text="Doppelzweier (2x)" key="race/boatClass/abbreviation___EQ___2x" />
          <ViewSettingsItem text="Zweier ohne St. (2-)" key="race/boatClass/abbreviation___EQ___2-" />
          <ViewSettingsItem text="Zweier mit St. (2+)" key="race/boatClass/abbreviation___EQ___2+" />
          <ViewSettingsItem text="Doppelvierer (4x)" key="race/boatClass/abbreviation___EQ___4x" />
          <ViewSettingsItem text="Vierer ohne St. (4-)" key="race/boatClass/abbreviation___EQ___4-" />
          <ViewSettingsItem text="Doppelvierer mit St. (4x+)" key="race/boatClass/abbreviation___EQ___4x+" />
          <ViewSettingsItem text="Vierer mit St. (4+)" key="race/boatClass/abbreviation___EQ___4+" />
          <ViewSettingsItem text="Achter mit St. (8+)" key="race/boatClass/abbreviation___EQ___8+" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.gender}" key="gender" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.male}" key="race/ageClass/gender___EQ___M" />
          <ViewSettingsItem text="{i18n>common.female}" key="race/ageClass/gender___EQ___W" />
          <ViewSettingsItem text="{i18n>common.mixed}" key="race/ageClass/gender___EQ___X" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.state}" key="cancelled" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.cancelled}" key="cancelled___EQ___true" />
          <ViewSettingsItem text="{i18n>heat.state.initial}" key="state___EQ___0" cust:filter="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.scheduled}" key="state___EQ___1" cust:filter="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.started}" key="state___EQ___2" cust:filter="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.finished}" key="state___EQ___5" cust:filter="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.official}" key="state___EQ___4" cust:filter="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.photoFinish}" key="state___EQ___6" cust:filter="cancelled___EQ___false" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.distance}" key="distance" multiSelect="false">
        <items>
          <ViewSettingsItem text="1000m" key="race/distance___EQ___1000" />
          <ViewSettingsItem text="1500m" key="race/distance___EQ___1500" />
          <ViewSettingsItem text="350m" key="race/distance___EQ___350" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.lightweight}" key="lightweight" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.yes}" key="race/lightweight___EQ___true" />
          <ViewSettingsItem text="{i18n>common.no}" key="race/lightweight___EQ___false" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.seeded}" key="seeded" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.yes}" key="race/seeded___EQ___true" />
          <ViewSettingsItem text="{i18n>common.no}" key="race/seeded___EQ___false" />
        </items>
      </ViewSettingsFilterItem>
    </filterItems>
  </ViewSettingsDialog>
</core:FragmentDefinition>