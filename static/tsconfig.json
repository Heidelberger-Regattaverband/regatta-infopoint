{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "rootDir": "./webapp",
    "baseUrl": "./",
    "paths": {
      "de/regatta_hd/infoportal/*": [
        "webapp/*"
      ]
    },
    "composite": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "types": [
      "@openui5/types",
      "@types/leaflet"
    ]
  },
  "include": [
    "./webapp/**/*"
  ]
}