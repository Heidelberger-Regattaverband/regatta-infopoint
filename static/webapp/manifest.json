{
  "_version": "2.0.0",
  "start_url": "index.html",
  "sap.app": {
    "id": "de.regatta_hd.infoportal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{app.title}}",
    "description": "{{app.description}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "/icon/favicon.ico"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "async": true,
      "viewName": "de.regatta_hd.infoportal.view.App",
      "type": "XML",
      "id": "app"
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "dependencies": {
      "minUI5Version": "1.141.0",
      "libs": {
        "sap.ui.core": {},
        "sap.f": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "de.regatta_hd.infoportal.i18n.i18n",
          "supportedLocales": [
            "de",
            "en"
          ],
          "fallbackLocale": "de"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "path": "de.regatta_hd.infoportal.view",
        "controlId": "infoportalApp",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": [
            "startpage"
          ]
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "startpage",
          "target": "startpage"
        },
        {
          "pattern": "races",
          "name": "races",
          "target": "races"
        },
        {
          "pattern": "raceDetails/{raceId}",
          "name": "raceDetails",
          "target": "raceDetails"
        },
        {
          "pattern": "heats",
          "name": "heats",
          "target": "heats"
        },
        {
          "pattern": "heatDetails/{heatId}",
          "name": "heatDetails",
          "target": "heatDetails"
        },
        {
          "pattern": "clubs",
          "name": "clubs",
          "target": "clubs"
        },
        {
          "pattern": "clubDetails/{clubId}",
          "name": "clubDetails",
          "target": "clubDetails"
        },
        {
          "pattern": "athletes",
          "name": "athletes",
          "target": "athletes"
        },
        {
          "pattern": "athleteDetails/{athleteId}",
          "name": "athleteDetails",
          "target": "athleteDetails"
        },
        {
          "pattern": "map",
          "name": "map",
          "target": "map"
        },
        {
          "pattern": "schedule",
          "name": "schedule",
          "target": "schedule"
        },
        {
          "pattern": "scoring",
          "name": "scoring",
          "target": "scoring"
        },
        {
          "pattern": "statistics",
          "name": "statistics",
          "target": "statistics"
        },
        {
          "pattern": "monitoring",
          "name": "monitoring",
          "target": "monitoring"
        },
        {
          "pattern": "timestrip",
          "name": "timestrip",
          "target": "timestrip"
        }
      ],
      "targets": {
        "startpage": {
          "type": "View",
          "name": "Launchpad",
          "level": 0,
          "viewType": "XML"
        },
        "races": {
          "type": "View",
          "name": "RacesTable",
          "level": 1,
          "viewType": "XML"
        },
        "raceDetails": {
          "type": "View",
          "name": "RaceDetails",
          "level": 2,
          "viewType": "XML"
        },
        "heats": {
          "type": "View",
          "name": "HeatsTable",
          "level": 1,
          "viewType": "XML"
        },
        "heatDetails": {
          "type": "View",
          "name": "HeatDetails",
          "level": 2,
          "viewType": "XML"
        },
        "clubs": {
          "type": "View",
          "name": "ClubsTable",
          "level": 1,
          "viewType": "XML"
        },
        "clubDetails": {
          "type": "View",
          "name": "ClubDetails",
          "level": 2,
          "viewType": "XML"
        },
        "athletes": {
          "type": "View",
          "name": "AthletesTable",
          "level": 1,
          "viewType": "XML"
        },
        "athleteDetails": {
          "type": "View",
          "name": "AthleteDetails",
          "level": 2,
          "viewType": "XML"
        },
        "schedule": {
          "type": "View",
          "name": "ScheduleTable",
          "level": 1,
          "viewType": "XML"
        },
        "map": {
          "type": "View",
          "name": "Map",
          "level": 1,
          "viewType": "XML"
        },
        "scoring": {
          "type": "View",
          "name": "ScoringTable",
          "level": 1,
          "viewType": "XML"
        },
        "statistics": {
          "type": "View",
          "name": "Statistics",
          "level": 1,
          "viewType": "XML"
        },
        "monitoring": {
          "type": "View",
          "name": "Monitoring",
          "level": 1,
          "viewType": "XML"
        },
        "timestrip": {
          "type": "View",
          "name": "Timestrip",
          "level": 1,
          "viewType": "XML"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        },
        {
          "uri": "css/leaflet.css"
        },
        {
          "uri": "css/MarkerCluster.css"
        },
        {
          "uri": "css/MarkerCluster.Default.css"
        }
      ]
    }
  }
}
