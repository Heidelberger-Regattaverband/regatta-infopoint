<mvc:View controllerName="de.regatta_hd.infoportal.controller.Map" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
  height="100%">
  <mvc:content>

    <Page title="{i18n>launchpad.map.header}" class="sapUiNoContentPadding" showNavButton="true" navButtonPress=".onNavBack">
      <headerContent>
        <MenuButton icon="sap-icon://map" tooltip="{i18n>common.center}">
          <menu>
            <Menu>
              <MenuItem id="centerRegattaButton" text="{i18n>map.regattaLocations}" press=".onCenterButtonPress" />
              <MenuItem id="centerClubsButton" text="{i18n>map.clubLocations}" press=".onCenterButtonPress" />
            </Menu>
          </menu>
        </MenuButton>
      </headerContent>

      <content>
        <!-- <Panel height="100%"> -->
        <core:HTML content="&lt;div id=&quot;map&quot; /&gt;" />
        <!-- </Panel> -->
      </content>
    </Page>

  </mvc:content>
</mvc:View>