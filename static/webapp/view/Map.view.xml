<mvc:View controllerName="de.regatta_hd.infoportal.controller.Map" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
  xmlns:core="sap.ui.core">
  <mvc:content>

    <Page title="{i18n>launchpad.map.header}" class="sapUiNoContentPadding" showNavButton="true" navButtonPress="onNavBack">
      <headerContent>
        <Button tooltip="{i18n>common.refresh}" icon="sap-icon://refresh" press="onRefreshButtonPress" />
      </headerContent>

      <content>
        <l:VerticalLayout class="sapUiContentPadding" width="100%">
          <l:content>
            <core:HTML content="&lt;div id=&quot;map&quot; /&gt;" />
          </l:content>
        </l:VerticalLayout>
      </content>
    </Page>

  </mvc:content>
</mvc:View>