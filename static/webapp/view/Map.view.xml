  <mvc:content>

    <Page title="{i18n>launchpad.map.header}" class="sapUiNoContentPadding" showNavButton="true" navButtonPress=".onNavBack">
      <headerContent>
        <Button tooltip="{i18n>common.center}" icon="sap-icon://map" press=".onCenterButtonPress" />
      </headerContent>

      <content>
        <!-- <Panel height="100%"> -->
        <core:HTML content="&lt;div id=&quot;map&quot; /&gt;" />
        <!-- </Panel> -->
      </content>
    </Page>

  </mvc:content>
</mvc:View>