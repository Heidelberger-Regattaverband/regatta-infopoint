<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <ViewSettingsDialog confirm="onHandleFilterDialogConfirm">
    <filterItems>
      <ViewSettingsFilterItem text="{i18n>common.boatClass}" key="gender">
        <items>
          <ViewSettingsItem text="Einer (1x)" key="boatClass/abbreviation___EQ___1x" />
          <ViewSettingsItem text="Doppelzweier (2x)" key="boatClass/abbreviation___EQ___2x" />
          <ViewSettingsItem text="Zweier ohne St. (2-)" key="boatClass/abbreviation___EQ___2-" />
          <ViewSettingsItem text="Zweier mit St. (2+)" key="boatClass/abbreviation___EQ___2+" />
          <ViewSettingsItem text="Doppelvierer (4x)" key="boatClass/abbreviation___EQ___4x" />
          <ViewSettingsItem text="Vierer ohne St. (4-)" key="boatClass/abbreviation___EQ___4-" />
          <ViewSettingsItem text="Doppelvierer mit St. (4x+)" key="boatClass/abbreviation___EQ___4x+" />
          <ViewSettingsItem text="Vierer mit St. (4+)" key="boatClass/abbreviation___EQ___4+" />
          <ViewSettingsItem text="Achter mit St. (8+)" key="boatClass/abbreviation___EQ___8+" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.gender}" key="gender" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.male}" key="ageClass/gender___EQ___M" />
          <ViewSettingsItem text="{i18n>common.female}" key="ageClass/gender___EQ___W" />
          <ViewSettingsItem text="{i18n>common.mixed}" key="ageClass/gender___EQ___X" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.distance}" key="distance" multiSelect="false" />

      <ViewSettingsFilterItem text="{i18n>common.state}" key="cancelled" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.scheduled}" key="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.cancelled}" key="cancelled___EQ___true" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.lightweight}" key="lightweight" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.yes}" key="lightweight___EQ___true" />
          <ViewSettingsItem text="{i18n>common.no}" key="lightweight___EQ___false" />
        </items>
      </ViewSettingsFilterItem>

      <!-- <ViewSettingsFilterItem text="{i18n>common.seededLbl}" key="seeded" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.yes}" key="seeded___EQ___true" />
          <ViewSettingsItem text="{i18n>common.no}" key="seeded___EQ___false" />
        </items>
      </ViewSettingsFilterItem> -->
    </filterItems>
  </ViewSettingsDialog>
</core:FragmentDefinition>