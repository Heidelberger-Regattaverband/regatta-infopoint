<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <ViewSettingsDialog confirm=".onHandleFilterDialogConfirm">
    <filterItems>
      <ViewSettingsFilterItem text="{i18n>common.gender}" key="gender" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.male}" key="ageClass/gender___EQ___M" />
          <ViewSettingsItem text="{i18n>common.female}" key="ageClass/gender___EQ___W" />
          <ViewSettingsItem text="{i18n>common.mixed}" key="ageClass/gender___EQ___X" />
        </items>
      </ViewSettingsFilterItem>

      <ViewSettingsFilterItem text="{i18n>common.state}" key="cancelled" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.scheduled}" key="cancelled___EQ___false" />
          <ViewSettingsItem text="{i18n>heat.state.cancelled}" key="cancelled___EQ___true" />
        </items>
      </ViewSettingsFilterItem>

      <!-- <ViewSettingsFilterItem text="{i18n>common.seededLbl}" key="seeded" multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>common.yes}" key="seeded___EQ___true" />
          <ViewSettingsItem text="{i18n>common.no}" key="seeded___EQ___false" />
        </items>
      </ViewSettingsFilterItem> -->
    </filterItems>
  </ViewSettingsDialog>
</core:FragmentDefinition>