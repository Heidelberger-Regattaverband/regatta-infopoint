<mvc:View controllerName="de.regatta_hd.infopoint.controller.Heats" xmlns="sap.m" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc">
  <mvc:content>

    <Page title="Ergebnisse der {regatta>/title} - {parts: [{path: 'regatta>/start_date'}],formatter: '.formatter.dateLabel'} bis {parts: [{path: 'regatta>/end_date'}],formatter: '.formatter.dateLabel'}" class="sapUiNoContentPadding" showNavButton="true" navButtonPress="onNavBack">
      <content>

        <f:FlexibleColumnLayout id="flexibleColumnLayout" backgroundDesign="Solid">
          <f:beginColumnPages>
            <mvc:XMLView id="beginView" viewName="de.regatta_hd.infopoint.view.HeatsTable" />
          </f:beginColumnPages>
          <f:midColumnPages>
            <mvc:XMLView id="detailView" viewName="de.regatta_hd.infopoint.view.HeatRegistrationsTable" />
          </f:midColumnPages>
        </f:FlexibleColumnLayout>

      </content>
    </Page>

  </mvc:content>
</mvc:View>