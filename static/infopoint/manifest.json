{
  "_version": "1.12.0",
  "sap.app": {
    "id": "de.regatta_hd.infopoint",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{app.title}}",
    "description": "{{app.description}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "/icon/favicon.ico"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "de.regatta_hd.infopoint.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.105.0",
      "libs": {
        "sap.ui.core": {},
        "sap.f": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "de.regatta_hd.infopoint.i18n.i18n",
          "supportedLocales": [
            "de",
            "en"
          ],
          "fallbackLocale": ""
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "de.regatta_hd.infopoint.view",
        "controlId": "infopointApp",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": [
            "startpage"
          ]
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "startpage",
          "target": "startpage"
        },
        {
          "pattern": "heats",
          "name": "heats",
          "target": "heats"
        },
        {
          "pattern": "scoring",
          "name": "scoring",
          "target": "scoring"
        },
        {
          "pattern": "races",
          "name": "races",
          "target": "races"
        }
      ],
      "targets": {
        "startpage": {
          "viewName": "Launchpad",
          "viewLevel": 0
        },
        "heats": {
          "viewName": "Heats",
          "viewLevel": 1
        },
        "scoring": {
          "viewName": "ScoringTable",
          "viewLevel": 1
        },
        "races": {
          "viewName": "RacesTable",
          "viewLevel": 1
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}